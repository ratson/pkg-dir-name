{
  "name": "pkg-dir-name",
  "version": "0.0.0",
  "description": "Find name from closest package.json or its directory name.",
  "scripts": {
    "lint": "eslint --ext js,md .",
    "test": "ava",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "read-pkg-up": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "eslint": "^4.3.0",
    "eslint-config-concise": "^0.10.0",
    "eslint-config-concise-ava": "^0.10.0"
  },
  "repository": "ratson/pkg-dir-name",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "*.md",
    "index.js"
  ],
  "keywords": [
    "pkg",
    "package",
    "file",
    "find",
    "up",
    "find-up",
    "findup",
    "look-up",
    "look",
    "search",
    "match",
    "resolve",
    "parent",
    "parents",
    "folder",
    "directory",
    "dir",
    "walk",
    "walking",
    "path",
    "name"
  ]
}
